<AnchorPane id="AnchorPane" fx:id="viewQuadro" onMouseEntered="#viewQuadroEntered" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TelaVisualizarQuadroHorarioController">
   <children>
      <Label layoutX="423.0" layoutY="74.0" text="Quadro de Horário">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <TableView fx:id="tableQuadroHorario" layoutX="115.0" layoutY="182.0" prefHeight="440.0" prefWidth="971.0">
         <columns>
            <TableColumn fx:id="columnID" prefWidth="70.0" text="ID" />
            <TableColumn fx:id="columnNome" prefWidth="150.0" text="Nome" />
            <TableColumn fx:id="columnDescricao" prefWidth="150.0" text="Descrição" />
            <TableColumn fx:id="columnHora" prefWidth="100.0" text="Hora" />
            <TableColumn fx:id="columnDIa" prefWidth="100.0" text="Dia" />
            <TableColumn fx:id="columnDuracao" prefWidth="100.0" text="Duração" />
            <TableColumn fx:id="columnUsuarios" prefWidth="300.0" text="Usuários" />
         </columns>
      </TableView>
      <Button fx:id="btnVoltar" layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onAction="#btnVoltarClick" prefHeight="30.0" prefWidth="150.0" text="Voltar" />
   </children>
</AnchorPane>
