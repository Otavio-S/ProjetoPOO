<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TelaRequerimentoController">
   <children>
      <Label layoutX="422.0" layoutY="114.0" text="Requerer Relatório">
         <font>
            <Font name="System Bold" size="40.0" />
         </font>
      </Label>
      <GridPane layoutX="361.0" layoutY="233.0" prefHeight="252.0" prefWidth="478.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="232.0" minWidth="10.0" prefWidth="149.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="329.0" minWidth="10.0" prefWidth="329.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Colaborador:" GridPane.rowIndex="1" />
            <Label text="Usuário:" GridPane.rowIndex="2" />
            <Label text="Coordenador:" />
            <ChoiceBox fx:id="cbCoordenador" prefHeight="31.0" prefWidth="386.0" GridPane.columnIndex="1" />
            <ChoiceBox fx:id="cbColaborador" prefHeight="31.0" prefWidth="425.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <ChoiceBox fx:id="cbUsuario" prefHeight="31.0" prefWidth="473.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Button fx:id="btnOk" layoutX="626.0" layoutY="526.0" mnemonicParsing="false" onAction="#btnOkClick" prefWidth="150.0" text="OK" />
      <Button fx:id="btnVoltar" layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onAction="#btnVoltarClick" prefHeight="30.0" prefWidth="150.0" text="Voltar" />
      <Button fx:id="btnCarregar" layoutX="425.0" layoutY="526.0" mnemonicParsing="false" onAction="#btnCarregarClick" prefWidth="150.0" text="Carregar Dados" />
   </children>
</AnchorPane>
